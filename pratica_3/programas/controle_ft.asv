clear
clc

%Parametros simulacao
t_i=0;
t_f=10;
t = linspace(ti,tf,10000);

%Parametros do motor
J = 0.01;
b = 0.1; 
K = 0.01;
R = 1; 
L = 0.5;

%Função de transferencia
tf=tf(K,[L*J,(R*J)+(L*b),b*R+K^2]);

%Apresentacao dos resultados
%Resposta ao degrau
[y,t] = step(tf,t_f);
%Resposta ao impulso
[y,t] = impulse(tf,t_f);

